/* pages/join/join.wxss */
.container{
  width:100%;
  display:flex;
  flex-direction: column;
  align-items: center;
}
.container-join-tbox{
  width:100%;
  height: 600rpx;
  background: #f0f0f0;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center
}
.join-enlish{
  line-height: 80rpx;
  font-size: 30px;
  text-align: center;
  color: #c0c0c0;
  margin: 80rpx 0 5rpx 0;
  font-weight: 700;
}
.join-chinese{
  font-size: 19px;
  text-align: center;
  color: #999;
}
.join-line{
  width: 70rpx;
  height: 5px;
  border-bottom: 2px solid red;
}
.join-summary{
  text-align: center;
  margin-top: 50rpx;
  line-height: 20px;
  font-size: 12px;
  padding: 5rpx 40rpx;
}