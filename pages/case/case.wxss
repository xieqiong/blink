.video-class{
  width: 100%;
}
.img-class{
  width:100%;
}
.container{
  width:100%;
  display:flex;
  flex-direction: column;
  align-items: center;
  background:rgb(25,25,25);
  color:#fff;
}
.more-class{
  padding-top: 70rpx;
  color:  rgb(133,133,133);
  font-size: 20px;
  padding-bottom: 20rpx;
}
.case-title{
  width: 100%;
  color: #fff;
  text-align: center;
  font-size: 20px;
  padding-top:40rpx;
}
.info-img{
  padding: 20rpx 30rpx
}
.sub-case-title{
  font-size: 14PX;
  color: rgb(133,133,133);
  padding-bottom:40rpx;
}
.videoCss{
  width: 100%;
}
.more-line{
  width: 20rpx;
  border: 1px solid rgb(133,133,133);
  margin-bottom:40rpx;
}
.more-case{
  width: 100%;
  min-height: 100rpx;
}
.case-container{
  display: flex;
  flex-wrap: wrap;
  width: calc(100% - 40rpx);
  margin-left:20rpx;
}
.content-case-item{
  width: calc(100% - 20rpx);
  display: block;
  padding: 10rpx 0;
  overflow: hidden;
}
.content-case{
  width: 50%;
  height: 300rpx;
  overflow: hidden;
}
.btn-view{
  margin-top: 40rpx;
  margin-bottom:100rpx;
}
.btn-inner{
  font-size: 12px;
  font-weight: 600;
  position: absolute;
  text-align: center;
  top: 6px;
  left: 30px;
}
.btn-inner-1{
  font-size: 12px;
  font-weight: 600;
  position: absolute;
  top:21px;
  left: 40px;
}
.btn {
  width: 130px;
  height: 50px;
  line-height: 20px;
  background-color:  rgb(25,25,25);
  color: #fff;
  border: 1px solid #fff;
  border-radius: 0;
  font-size: 0;
}
.content-item-img{
  width: 100%;
  height: 200rpx;
}
.content-item-title{
  font-size:12px;
  line-height:30rpx;
  color: #666;
  padding-right: 20rpx;
}
.content-item-title-sub{
  font-size:9px;
  color: #999;
  padding-right: 20rpx;
  line-height: 25rpx;
}